/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  /* padding: 200rpx 0; */
  box-sizing: border-box;
}
.content{
  height: 100%;
  font-size: 24rpx;
}
page{
  height: 100%;
  background: #f5f4f4;
}
.ellipsis{
  display: block;
  overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.arrow-right{
  position: relative;
}
.arrow-right::after{
  content: "";
  position: absolute;
  right: 26rpx;
  top: 50%;
  width: 28rpx;
  height: 28rpx;
  margin-top: -14rpx;
  border: 2rpx solid #888;
  border-left: none;
  border-bottom: none;
  transform: rotate(45deg);
}
.mb18{
  margin-bottom: 18rpx;
}